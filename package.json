    "@aws-sdk/client-s3": "^3.633.0",
    "@cori-risi/graphql-schemas": "file:../graphql-schemas",
    "@graphiql/toolkit": "^0.10.0",
    "@graphql-tools/stitch": "^8.7.50",
    "adm-zip": "^0.5.10",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-core": "^3.13.0",
    "apollo-server-lambda": "^3.13.0",
    "apollo-server-plugin-response-cache": "^3.8.2",
    "aws-cdk-lib": "^2.115.0",
    "child_process": "^1.0.2",
    "compression": "^1.7.4",
    "constructs": "^10.3.0",
    "dotenv": "^16.3.1",
    "graphiql": "^3.7.0",
    "graphql": "^16.8.1",
    "graphql-compose": "^9.0.10",
    "graphql-tools": "^8.3.20",
    "ioredis": "^5.3.2",
    "nanoid": "^3.3.7",
    "nodemon": "^3.1.4",
    "react": "",
    "react-dom": ""
  },
  "devDependencies": {
    "@aws-amplify/analytics": "^7.0.7",
    "@graphql-hive/cli": "^0.12.0",
