    "@graphql-tools/load": "^7.8.14",
    "@graphql-tools/schema": "^8.5.1",
    "@ioredis/commands": "^1.2.0",
    "@types/adm-zip": "^0.5.5",
    "@types/compression": "^1.7.5",
    "@types/graphql": "^14.5.0",
    "@types/ip": "^1.1.3",
    "@types/ncp": "^2.0.8",
    "@types/node": "^22.4.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^4.3.1",
    "apollo-server": "^3.13.0",
    "aws-cdk": "^2.99.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.8",
    "diagnose-endpoint": "^1.1.0",
    "express": "^4.18.2",
    "flatted": "^3.3.1",
    "graphql": "^16.8.1",
    "graphql-tools": "^8.3.20",
    "jest": "^29.7.0",
    "node-ip": "^0.1.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "util": "^0.12.5",
    "uuid": "^8.3.2",
    "vite": "^5.4.1",
    "yaml": "^2.3.4"
  },
  "scripts": {
    "build": "npm run tsc && npm run build:merge-schemas && npm run build:graphiql",
    "build:graphiql": "vite build",
    "build:readme": "quarto render R/README.qmd --to gfm --output README.md",
    "build:dev": "npm run tsc -- --watch",
    "build:merge-schemas": "ts-node -P tsconfig.node.json index.ts",
    "build:schemas": "npm run build:merge-schemas",
    "clean": "rimraf dist",
    "dev": "npm run dev:merge-schemas & vite",
    "dev:merge-schemas": "npm run build:dev & npm run ts ./merge-schemas.ts",
    "format": "prettier --write .",
    "hive-publish": "npx hive schema:publish dist/schema.graphql --service api --force",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node dist/server.js",
    "test": "vitest",
    "ts": "npm install cross-env nodemon ts-node && cross-env nodemon '--watch' schema ",
    "tsc": "tsc -p tsconfig.node.json",
    "type:check": "ts --noEmit"
  }
}
